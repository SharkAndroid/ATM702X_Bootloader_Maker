	<mapping function = "pwm" group = "pwm2_ksout2" boot_enable = "true"/>
	<mapping function = "uart2" />

	<mapping function = "sd0" group = "sd0" dev_name = "asoc-mmc0.0" state_name = "sd0_host"/>
	<mapping function = "uart5" group = "uart5_sd0" dev_name = "asoc-mmc0.0" state_name = "serial_port"/>

	<mapping function = "sd1" group = "sd1" dev_name = "asoc-mmc1.0" state_name = "sd1_wifi"/>
</pinctrl_mapping>
